{
  "buildCommand": "pip install -r requirements.txt",
  "builds": [
    {
      "src": "app/**/*",
      "use": "@vercel/next"
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 0 * * *"
    }
  ],
  "routes": [
    {
        "src": "/api/cron",
        "methods": ["GET"],
        "dest": "/api/cron/route.ts"
    }
  ]
}